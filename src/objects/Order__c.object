    <validationRules>
        <fullName>CompareOrderPlaceOrderShipDate</fullName>
        <active>true</active>
        <errorConditionFormula>Order_Placed_Date__c &gt;Order_Ship_Date__c</errorConditionFormula>
        <errorDisplayField>Order_Placed_Date__c</errorDisplayField>
        <errorMessage>OrderPlace Date is greater than OrderShipDate</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>InvalidOrderStates</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Customer__r.State__c ,&quot;Maharashtra&quot;) ||   ISPICKVAL( Customer__r.State__c ,&quot;Gujrat&quot;)</errorConditionFormula>
        <errorMessage>Orders should not be placed from Customers belonging to Gujarat and Maharashtra and should give an error</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NoEmptyOrderQuantity</fullName>
        <active>true</active>
        <errorConditionFormula>Order_Quantity__c &lt;=0</errorConditionFormula>
        <errorDisplayField>Order_Quantity__c</errorDisplayField>
        <errorMessage>Order quantity should always be greater than 0</errorMessage>
    </validationRules>
